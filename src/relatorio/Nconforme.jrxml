	<parameter name="dinicio" class="java.sql.Date"/>
	<parameter name="dfinal" class="java.sql.Date"/>
	<queryString>
		<![CDATA[SELECT 
count( itens.inspecao)as qtd,
itens.inspecao,
 ris.nome as nomeRis,
 obra.nome as nomeObra  

FROM 
  pitem 
  inner join itens on itens.id = pitem.itens_id
  inner join obra on obra.id = pitem.obraid
  inner join ris on ris.id = itens.ris_id
  where pitem.conformidade =0 
and pitem.data > $P{dinicio} 
and pitem.data < $P{dfinal} 

  group by ris.nome, 
  itens.inspecao, 
  obra.nome]]>
	</queryString>
