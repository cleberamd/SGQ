	<parameter name="dinicio" class="java.sql.Date"/>
	<parameter name="dfinal" class="java.sql.Date"/>
	<parameter name="obra" class="java.lang.String"/>
	<queryString>
		<![CDATA[select ob.nome as obraNome,
ob.numero as ccusto,
CASE pi.conformidade
     when 0 then 'não conforme'
    when 2 then 'conforme após revisão'
  
 
    when 1 then 'conforme'  
    end  as conformidade,
pi.data,
ris.nome as risNome,
it.inspecao,
pi.descricao,
pi.foto
 from pitem pi 
inner join itens it on (it.id = pi.itens_id)
inner join ris on (ris.id = it.ris_id)
inner join obra ob on (ob.id = pi.obraid)
where pi.data >=  $P{dinicio} and pi.data <=  $P{dfinal} 
and ob.nome like  $P{obra} 
order by pi.data]]>
	</queryString>
